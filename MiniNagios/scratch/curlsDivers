curl -X POST http://localhost:3000/api/serveurs ^
     -H "Content-Type: application/json" ^
     -d "{\"hostname\": \"SRV-TEST\", \"ip\": \"192.168.0.50\", \"mac\": \"A1:B2:C3:D4:E5:F6\", \"os\": \"Ubuntu\"}"


     curl -X POST http://localhost:3000/api/routeurs -H "Content-Type: application/json" -d "{\"hostname\": \"RT-ROB\", \"ip\": \"192.168.0.2\", \"mac\": \"A1:B2:C3:D4:E5:F6\", \"nbPorts\": 28}"

     curl -X PATCH http://localhost:3000/api/equipements/0/plug